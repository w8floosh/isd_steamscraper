  "devDependencies": {
    "@dotenvx/dotenvx": "^0.27.0",
    "@quasar/app-vite": "^1.4.3",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
