{
  "name": "client",
  "version": "0.0.1",
  "description": "Quasar client for SteamScraper APIs",
  "productName": "SteamScraper",
  "author": "Martin <gibilterramartin@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
